    "build": "tsc -p tsconfig.build.json",
    "build:render": "npm install --save-dev @types/express@4.17.21 @types/express-session@1.18.1 @types/passport@1.0.16 @types/passport-local@1.0.38 @types/express-fileupload@1.5.1 @types/connect-pg-simple@7.0.3 @types/ws@8.5.13 @types/nodemailer && tsc -p no-vite.tsconfig.json --skipLibCheck && node fix-all-imports.cjs",
    "start": "NODE_ENV=production node dist/standalone.js",
    "db:generate": "drizzle-kit generate:pg --schema=./db/schema.ts --out=./migrations",
    "db:migrate": "tsx db/migrate.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@neondatabase/serverless": "0.10.4",
    "@sendgrid/mail": "^8.1.4",
    "@types/express": "4.17.21",
