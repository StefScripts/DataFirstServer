    "neon-serverless": "0.5.3",
    "node-cache": "5.1.2",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "psql": "^0.0.1",
    "ts-node": "^10.9.1",
    "typescript": "5.6.3",
