    "compression": "1.8.0",
    "connect-pg-simple": "^10.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.7",
    "drizzle-kit": "0.30.5",
    "drizzle-orm": "0.40.0",
    "drizzle-zod": "0.7.0",
    "express": "^4.21.2",
